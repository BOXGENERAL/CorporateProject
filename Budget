public class Budget extend Department
{
	protected int year;
	protected int month;
	protected double total;
	protected Category [] categoryList = new Category[50];
	//Contstructor
	public Budget ()
	{
		year = Corporate.GetTodayYear();
		month = Corporate.GetTodayMonth();
		total = 0;
	}
	public void calTotal ()
	{

	
	}
	public double getTotal()
	{
	
		return total;
	}
	public void compareBudget(Budget other)
	{
	
		return this.Budget-other.Budget;
	}
	public String toString ()
	{
		return ("year "+year+" month"+" total"+total);
	}
}
