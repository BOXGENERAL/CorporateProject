public class calculateMonth
{
   public static void main (String []args)
   {
   //Change the variables if neccessary these are just the algorithms to calculate
   int currentyear = 2017;
   int currentmonth = 3;
   int futureyear = 2019;
   int futuremonth = 5;
   int totalmonth;
   int totalyear;
   //These are the calculations neccessary
   currentyear = currentyear + 1;
   totalyear = futureyear - currentyear;
   totalmonth = ((totalyear*12) + (13 - currentmonth) + (futuremonth));
   
   
   }


}
