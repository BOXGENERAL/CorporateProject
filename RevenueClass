class Revenue
{
	protected int year;
	protected int month;
	protected double money;
	
	//Constructor 
	public Revenue (int year,int month, double money)
	{
		//The whole process breaks down the time in year month
		//The proccess of Calculating year
		this.year = year;
		this.month = month;
		this.money = money;
	}//Revenue method
	
	//CompareTo method that compares the Revenue class
	public double compareRevenue (Revenue other)
	{
		return this.money - other.money;
	
	
	}//compareRevenue method
	
	//Displays a report on the Revenue
	public void toString ()
	{
		System.out.print ("Year: " + year + " Month: " + month + "Revenue " + money);
	}//toString method
}//Revenue.class
