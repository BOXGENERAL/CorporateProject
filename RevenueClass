class Revenue
{
	protected int year;
	protected int month;
	protected double money;
	
	//Constructor 
	public Revenue (int time, double money)
	{
		//The whole process breaks down the time in year month
		//The proccess of Calculating year
		int temp;
		temp = time/100;
		this.year = temp;
		//The proccess of Calculating month
		int count = 10000;
		temp = time;
		for (int i = 0; i < 3;i ++)
		{
			while (temp > count)
			{
				temp = temp = count;
			}
			count = count / 10
		
		}
		this.month = temp;
		this.money = money;
	}//Revenue method
	
	//CompareTo method that compares the 
	public double compareRevenue (Revenue other)
	{
		return this.money - other.money;
	
	
	}//compareRevenue method
	
	//Displays a report on the Revenue
	public void toString ()
	{
		System.out.print ("Year: " + year + " Month: " + month + "Revenue " + money);
	}//toString method
}//Revenue.class
